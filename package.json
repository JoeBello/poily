{
  "name": "poily",
  "version": "0.0.0",
  "engines": {
    "node": ">=4.0.0",
    "npm": ">=3.0.0"
  },
  "private": true,
  "scripts": {
    "build:dev": "npm-run-all clean webpack --env=dev",
    "build:prod": "npm-run-all clean webpack --env=prod",
    "clean": "rimraf ./src/dist",
    "dev:server": "NODE_ENV=dev nodemon index.js",
    "dev": "concurrently --kill-others \"npm run dev:server\" \"npm run webpack-server\"",
    "heroku-postbuild": "webpack --env=prod",
    "live": "NODE_ENV=prod node index.js",
    "prod:server": "NODE_ENV=prod nodemon index.js",
    "webpack-server": "webpack-dev-server --env=dev"
  },
  "dependencies": {
    "body-parser": "^1.17.1",
    "dotenv": "^4.0.0",
    "express": "~4.13.4",
    "googleplaces-promises": "^1.0.0",
    "helmet": "^3.6.0",
    "hpp": "^0.2.2",
    "morgan": "^1.8.1",
    "node-geocoder": "^3.15.0"
  },
  "devDependencies": {
    "@uirouter/angularjs": "^1.0.3",
    "angular": "^1.6.4",
    "angular-animate": "^1.6.4",
    "angular-ui-bootstrap": "^2.5.0",
    "bootstrap-sass": "^3.3.7",
    "bootswatch": "^3.3.7",
    "concurrently": "^3.3.0",
    "css-loader": "^0.26.1",
    "debug": "^2.6.6",
    "extract-text-webpack-plugin": "^2.0.0-rc.3",
    "file-loader": "^0.10.0",
    "font-awesome": "^4.7.0",
    "html-loader": "^0.4.4",
    "ng-annotate-webpack-plugin": "^0.2.1-pre",
    "ngstorage-webpack": "^2.2.2",
    "node-sass": "^4.5.2",
    "npm-run-all": "^4.0.2",
    "progress-bar-webpack-plugin": "^1.9.3",
    "q": "^1.5.0",
    "rimraf": "^2.6.0",
    "sass-loader": "^6.0.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.1"
  }
}
